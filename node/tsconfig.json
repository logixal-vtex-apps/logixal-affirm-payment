{
    "compilerOptions": {
      "types": [
        "node"
      ],
      "typeRoots": [
        "node_modules/@types"
      ],
      "target": "es6",
      "module": "commonjs",
      "sourceMap": false,
      "allowJs": true,
      "allowSyntheticDefaultImports": true
    },
    "typeAcquisition": {
      "enable": false
    },
    "include": [
      "./**/*"
    ],
    "exclude": [
      "node_modules"
    ],
    "compileOnSave": true
  }